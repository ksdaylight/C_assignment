#include <stdio.h>
#include <malloc.h>
int zi(int *n,int size){
    int i;
    for(i = 0; i < size; i ++)
    {
        if( *(n + i * size + i) != 1)
            return 0;
    }

    return 1;
}
int dui(int *n,int size)
{
    int i,j;
     for(i = 0;i < size ; i ++ )
    {
        for(j = 0; j < size; j++)
        if(*(n + i * size + j) == 1)
            if(*(n + i * size + j) != *(n + j * size + i))
                return 0;
    }
    return 1;
}
